<div class="container">
  <div class='col-md-5' style="width: 23.666667%;">
    <div class="form-group">
      <%= form_tag search_path(filename: 'trackers.csv', format: "csv") do %>
        From Date: <div class='input-group date' id='datetimepicker6'>
        <%#= date_field_tag :created_at %>
        <input id="fromdate" type='text' value="" name="created_at" class="form-control" data-behavior="datepicker">
        
        <span class="input-group-addon">
          <span class="glyphicon glyphicon-calendar"></span>
        </span>
        </div><br>
        To Date: <div class='input-group date' id='datetimepicker7'>
        <%#= date_field_tag :updated_at %>
        <input id="todate" type='text' value="" name="updated_at" class="form-control" data-behavior="datepicker">
        <span class="input-group-addon">
          <span class="glyphicon glyphicon-calendar"></span>
        </span>
        </div><br>
       <span><%= submit_tag 'Download Tracker', class: 'btn btn-success btn-lg' %></span>
      <% end %>         
      </div>
    </div>
  </div>
</div> 


<script type="text/javascript">
  $(document).ready(function(){
    $('#datepicker').datepicker();
    $('#datetimepicker6').datepicker({
    format: "yyyy-mm-dd"
      });
    $('#datetimepicker7').datepicker({
    format: "yyyy-mm-dd"
      });
  });

</script>